xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="furniture">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="furniture" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:byte" name="f_id"/>
              <xs:element type="xs:string" name="f_type"/>
              <xs:element type="xs:string" name="f_woodtype"/>
              <xs:element type="xs:string" name="f_description"/>
              <xs:element type="xs:string" name="f_color"/>
              <xs:element type="xs:int" name="f_price"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>